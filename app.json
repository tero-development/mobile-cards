      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#BCEBE8"
      },
      "package": "com.terodev.TeroLearning"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "02063917-111d-4f3d-8264-bccf2a86485b"
      }
    },
    "plugins": [
      [
        "expo-notifications",
        {
          "icon": "./assets/icon2.png",
          "color": "#ffffff"
        }
      ]
    ]
  }
}

